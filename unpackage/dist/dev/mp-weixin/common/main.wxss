  height: 54rpx;
  left: 32rpx;
  top: 32rpx;
}
.voice .voice-item.active .item-li:nth-child(2) {
  -webkit-animation: fade 1s infinite 0.2s;
          animation: fade 1s infinite 0.2s;
}
.voice .voice-item.active .item-li:nth-child(3) {
  -webkit-animation: fade 1s infinite 0.4s;
          animation: fade 1s infinite 0.4s;
}
@-webkit-keyframes fade {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@keyframes fade {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
