.voice .voice-item .item-li {
  position: absolute;
  border: 4rpx solid #000000;
  border-radius: 50%;
}
.voice .voice-item .item-li:nth-child(1) {
  width: 18rpx;
  height: 18rpx;
  left: 52rpx;
  top: 52rpx;
}
.voice .voice-item .item-li:nth-child(2) {
  width: 36rpx;
