<view class="content"><block wx:for="{{list}}" wx:for-item="e" wx:for-index="i" wx:key="i"><view class="default-cell" index="{{i}}" data-event-opts="{{[['tap',[['chat',['$0'],[[['list','',i]]]]]]]}}" bindtap="__e"><view class="cell-row"><view class="cell-avatar-tip"><image class="{{['cell-avatar']}}" src="{{e.avatar}}"></image><view hidden="{{!(e.chatCount)}}" class="tipShow">{{e.chatCount}}</view></view><view class="cell-item"><view class="cell-title">{{e.friend_remark}}</view><view class="cell-cont">{{e.end_cont}}</view></view><view class="cell-operation-tip">{{''+e.end_date+''}}</view></view></view></block><view hidden="{{!(false)}}">{{isTrue}}</view></view>